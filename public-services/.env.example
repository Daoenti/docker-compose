# Vaultwarden Admin Token
# Generate with: openssl rand -base64 48
VAULT_TOKEN=your_argon2_hashed_token_here

# Foundry VTT Credentials
F_USER=your_foundry_username
F_PASS=your_foundry_password
F_ADMIN_KEY=your_admin_key_here

# Cloudflare API Token for Caddy DNS Challenge
# Create at: https://dash.cloudflare.com/profile/api-tokens
# Required permissions: Zone > DNS > Edit, Zone > Zone > Read
CLOUDFLARE_API_TOKEN=your_cloudflare_api_token_here

# NVIDIA GPU Configuration
NVIDIA_VISIBLE_DEVICES=all
