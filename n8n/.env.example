# PostgreSQL Root User (for initial setup)
POSTGRES_USER=admin
POSTGRES_PASSWORD=your_secure_admin_password

# PostgreSQL Database Name
POSTGRES_DB=n8n

# PostgreSQL Application User (used by n8n)
POSTGRES_NON_ROOT_USER=n8n
POSTGRES_NON_ROOT_PASSWORD=your_secure_n8n_password

# n8n Encryption Key (generate a random string)
ENCRYPTION_KEY=your_random_encryption_key_here

# Node.js Built-in Modules (use * to allow all, or comma-separated list)
NODE_BUILTIN=*
